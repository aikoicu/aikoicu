{
  "name": "root",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap --config ./next-sitemap.js",
    "start": "next start",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "component": "cypress open --component",
    "component:headless": "cypress run --component",
    "lint": "pnpm lint:es && pnpm lint:css",
    "lint:ts": "tsc --pretty --project tsconfig.json --noEmit",
    "lint:es": "eslint -c ./.eslintrc.json --ignore-path \"../../.eslintignore\" .",
    "lint:es:fix": "eslint -c ./.eslintrc.json --ignore-path \"../../.eslintignore\" . --fix",
    "lint:css": "stylelint --config \"../../.stylelintrc.json\" -i \"../../.stylelintignore\"",
    "lint:css:fix": "stylelint --config \"../../.stylelintrc.json\" -i \"../../.stylelintignore\" --fix",
    "lint:fix": "pnpm formatter && pnpm lint:es:fix && pnpm lint:css:fix",
    "formatter": "prettier --config \"../../.prettierrc.json\" --ignore-path \"../../.prettierignore\" --write \"./src/**/*.{js,ts,tsx,json,css,md,yml,html,mdx}\"",
    "turbo:prune": "turbo prune --scope=root --docker",
    "analyze": "cross-env ANALYZE=true pnpm build"
  },
  "dependencies": {
    "@headlessui/react": "1.7.15",
    "@mdx-js/loader": "2.3.0",
    "@mdx-js/react": "2.3.0",
    "@next/bundle-analyzer": "13.4.7",
    "@next/mdx": "13.4.7",
    "@normy/react-query": "0.7.0",
    "@tailwindcss/typography": "0.5.9",
    "@tanstack/query-core": "4.29.15",
    "@tanstack/react-query": "4.29.15",
    "@tanstack/react-query-devtools": "4.29.15",
    "@types/inline-style-prefixer": "5.0.0",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "@types/ws": "8.5.5",
    "autoprefixer": "10.4.14",
    "clsx": "1.2.1",
    "cross-env": "7.0.3",
    "daisyui": "3.1.5",
    "eslint-plugin-cypress": "2.13.3",
    "framer-motion": "10.12.16",
    "graphql-ws": "5.14.0",
    "inline-style-prefixer": "7.0.0",
    "next": "13.4.7",
    "next-pwa": "5.6.0",
    "next-sitemap": "4.1.3",
    "next-themes": "0.2.1",
    "postcss": "8.4.24",
    "postcss-nesting": "11.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hotjar": "6.1.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3",
    "ws": "8.13.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "13.4.7",
    "@tanstack/eslint-plugin-query": "4.29.9",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.5.2",
    "cypress": "12.15.0",
    "jsdom": "22.1.0"
  },
  "peerDependencies": {
    "next": "*"
  },
  "browserslist": [
    ">0.2%",
    "last 2 versions",
    "not dead"
  ],
  "_moduleAliases": {
    "@/": "./src/",
    "public": "./public"
  },
  "packageManager": "pnpm@8.6.0"
}

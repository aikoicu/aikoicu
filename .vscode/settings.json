{
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "tailwindCSS.experimental.classRegex": [
    [
      "clsx\\(([^)]*\\n*[^)]*)\\)",
      "addGlassStyle\\(([^)]*\\n*[^)]*)\\)",
      "(?:'|\"|`)([^']*)(?:'|\"|`)"
    ],
    "(?:enter|leave)(?:From|To)?=\\s*(?:\"|')([^(?:\"|')]*)"
  ],
  "cSpell.words": [
    "aiko",
    "aikoicu",
    "Boosty",
    "clsx",
    "codeguide",
    "commitlint",
    "csstree",
    "daisyui",
    "evilmartians",
    "Fira",
    "headlessui",
    "lefthook",
    "Lekton",
    "Maxx",
    "msapplication",
    "mstile",
    "nextui",
    "normy",
    "onlyfans",
    "popmotion",
    "postbuild",
    "prefixer",
    "preinstall",
    "stylelint",
    "stylelintignore",
    "stylelintrc",
    "tailwindcss",
    "tanstack"
  ],
  "svg.preview.background": "dark-transparent",
}
